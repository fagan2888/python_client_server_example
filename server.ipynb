      "from flask import Flask, request, jsonify\n",
      "import pandas as pd\n",
      "\n",
      "app = Flask(__name__)\n",
      "\n",
      "@app.route('/load', methods=['PUT'])\n",
      "def load():\n",
      "    df = pd.read_json(request.json)\n",
      "    print len(df)\n",
      "    return jsonify({'numrecs': len(df)})\n",
      "\n",
      "app.run()"
